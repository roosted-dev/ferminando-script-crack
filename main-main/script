local page = venyx:addPage("스크립트", 5012544693)
local script = page:addSection("스크립트")
local ID = page:addSection("게임 ID")


script:addButton("투명", function()
    loadstring(game:HttpGet("https://pastebin.com/raw/iiKMg6iw"))();
end)
script:addButton("채팅 조작", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/ferminando/script/main/chat%20hax"))()
end)
script:addButton("Simple Spy (Synpase x, krnl만 가능)", function()
    loadstring(game:HttpGet("https://github.com/exxtremestuffs/SimpleSpySource/raw/master/SimpleSpy.lua"))()
end)
script:addButton("안티 잠수", function()
    local vu = game:GetService("VirtualUser")
game:GetService("Players").LocalPlayer.Idled:connect(function()
   vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
   wait(1)
   vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
    end)
end)
script:addButton("히트박스", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/2dgeneralspam1/scripts-and-stuff/master/shit%20script%20pack/CheatX'))()
end)

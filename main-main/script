local fe = venyx:addPage("FE 스크립트", 5012544693)
local fescript = fe:addSection("스크립트")
local fedp = fe:addSection("플레이어")
local feui = fe:addSection("창")

fescript:addButton("Fe 물건 움직이기", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/ferminando/script/main/change%20position"))();
end)
fescript:addButton("Fe 물건 움직이기 2", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/ferminando/script/main/change%20position%202"))();
end)
fescript:addButton("Fe 벽 걷기", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/ferminando/script/main/walk%20wall"))();
end)
fescript:addButton("Fe 트롤링", function()
    loadstring(game:HttpGet("https://pastebin.com/raw/bk5J4cyc"))();
end)
fescript:addButton("Fe 날려서 죽이기", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/ferminando/script/main/fe%20filling%20kill'))();
end)
local PlayList = {}
for i,v in pairs(game:GetService("Players"):GetPlayers()) do 
    if v ~= game.Players.LocalPlayer then 
        table.insert(PlayList,v.Name)
    end
end

--   teleport way
fedp:addDropdown("Players",PlayList,function(ok)
    xdvalue = ok
end)
fedp:addButton("플레이어 목록 초기화", function()
    table.clear(PlayList)
    wait(0.1)
    for i,v in pairs(game:GetService("Players"):GetPlayers()) do 
        if v ~= game.Players.LocalPlayer then 
            table.insert(PlayList,v.Name)
        end
    end
end)

fedp:addButton("상대 대리고 오기 (아이템 있어야함)", function()

    Target = xdvalue
    NOW = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
game.Players.LocalPlayer.Character.Humanoid.Name = 1
local l = game.Players.LocalPlayer.Character["1"]:Clone()
l.Parent = game.Players.LocalPlayer.Character
l.Name = "Humanoid"
wait(0.1)
game.Players.LocalPlayer.Character["1"]:Destroy()
game.Workspace.CurrentCamera.CameraSubject = game.Players.LocalPlayer.Character
game.Players.LocalPlayer.Character.Animate.Disabled = true
wait(0.1)
game.Players.LocalPlayer.Character.Animate.Disabled = false
game.Players.LocalPlayer.Character.Humanoid.DisplayDistanceType = "None"
for i,v in pairs(game:GetService'Players'.LocalPlayer.Backpack:GetChildren())do
game.Players.LocalPlayer.Character.Humanoid:EquipTool(v)
end
local function tp(player,player2)
local char1,char2=player.Character,player2.Character
if char1 and char2 then
char1.HumanoidRootPart.CFrame = char2.HumanoidRootPart.CFrame
end
end
local function getout(player,player2)
local char1,char2=player.Character,player2.Character
if char1 and char2 then
char1:MoveTo(char2.Head.Position)
end
end
tp(game.Players[Target], game.Players.LocalPlayer)
wait(0.1)
tp(game.Players[Target], game.Players.LocalPlayer)
wait(0.3)
getout(game.Players.LocalPlayer, game.Players[Target])
wait(0.4)
local sav = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
wait(4)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = sav

end)
fedp:addButton("텔레포트",function()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players[xdvalue].Character.HumanoidRootPart.CFrame 
end)
feui:addButton("Fe 날려서 죽이기 창 없애기", function()
    game.CoreGui["fillingkill"]:Destroy()
end)
feui:addButton("Fe 트롤링 창 없애기", function()
    game.CoreGui["OPFinality"]:Destroy()
end)

local es = venyx:addPage("ESP", 5012544693)
local esp = es:addSection("ESP 설정")

esp:addToggle("ESP", nil, function(d)
    ESP:Toggle(d)
end)
esp:addToggle("같은 팀 보이기", nil, function(e)
    ESP.TeamMates = e
end)

local setting = venyx:addPage("플레이어", 5012544693)
local player = setting:addSection("플레이어 설정")


player:addSlider("속도", 16, 16, 250, function(v)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = (v)
end)
player:addSlider("점프파워", 50, 50, 250, function(v)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = (v)
end)
player:addSlider("중력", 200, 0, 200, function(v)
    game.Workspace.Gravity = (v)
end)
player:addButton("리조인", function()
    game:GetService("TeleportService"):Teleport(game.PlaceId, game:GetService("Players").LocalPlayer)
end)

player:addToggle("자동 리스폰", nil, function(v)
    getgenv().spw = v
    while wait() do
    if not getgenv().spw then return end
    if game.Players.LocalPlayer.Character.Humanoid.Health < 1 and swit == false then
        poi = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
        swit = true
    end

    if swit == true then
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = poi
        end
    game.Players.LocalPlayer.CharacterAdded:connect(function()
        wait(0.6)
        swit = false
    end)
end
end)

-- second page
local set = venyx:addPage("설정", 5012544693)
local colors = set:addSection("색깔")
local ui = set:addSection("창")


for theme, color in pairs(themes) do -- all in one theme changer, i know, im cool
colors:addColorPicker(theme, color, function(color3)
venyx:setTheme(theme, color3)
end)
end

ui:addButton("스크립트 허브 창 없애기", function()
    game.CoreGui["Script Hub by Ferminando"]:Destroy()
    game.CoreGui["AkaliNotif"]:Destroy()
end)
ui:addKeybind("창 껏다 키기", Enum.KeyCode.RightControl, function(v)
    venyx:toggle(v)
end)
-- load
venyx:SelectPage(venyx.pages[1], true)
